<?xml version="1.0"?>
<mugl>
  <graph width="400" height="150">
    <window margin="0" padding="0" border="0"/>
    <horizontalaxis id="xaxis" type="number" min="0" max="10">
      <title>x</title>
      <labels format="%1d" start="0" spacing="5 3 1" />
      <binding id="xbinding" min="0" max="1"/>
    </horizontalaxis>
    <verticalaxis id="yaxis" type="number" min="0" max="10">
      <title position="-26 0">y</title>
      <labels format="%.1f" start="1" spacing="50 20 10 5 2 1 0.5 0.05"/>
    </verticalaxis>
    <plot>
      <renderer type="line">
	<option name="linecolor" value="blue"/>
	<option name="dotcolor"  value="black"/>
	<option name="dotsize"   value="3"/>
	<option name="linewidth" value="2"/>
      </renderer>
    </plot>
    <data>
      <variables>
	<variable id="xvar" column="0" type="number"/>
	<variable id="yvar" column="1"/>
	<variable id="zvar" column="2"/>
      </variables>
      <values>
	0, 3
	1, 2
	2, 4
	3, 5
	4, 3
	5, 6
	6, 7
	7, 8
	8, 5
	9, 4
	10,5
      </values>
    </data>
  </graph>
  <graph y="150" width="400" height="150">
    <window margin="0" padding="0" border="0"/>
    <horizontalaxis id="xaxis2" type="number" min="0" max="10">
      <title>x</title>
      <labels format="%1d" start="0" spacing="5 3 1" />
      <binding id="xbinding" min="0" max="1"/>
    </horizontalaxis>
    <verticalaxis id="yaxis2" type="number" min="0" max="10">
      <title position="-26 0">y</title>
      <labels format="%.1f" start="1" spacing="50 20 10 5 2 1 0.5 0.05"/>
    </verticalaxis>
    <plot>
      <renderer type="line">
	<option name="linecolor" value="red"/>
	<option name="dotcolor"  value="black"/>
	<option name="dotsize"   value="3"/>
	<option name="linewidth" value="2"/>
      </renderer>
    </plot>
    <data>
      <variables>
	<variable id="xvar2" column="0" type="number"/>
	<variable id="yvar2" column="1"/>
	<variable id="zvar2" column="2"/>
      </variables>
      <values>
	0, 4
	1, 5
	2, 6
	3, 7
	4, 1
	5, 3
	6, 2
	7, 9
	8, 7
	9, 6
	10,4
      </values>
    </data>
  </graph>
</mugl>
