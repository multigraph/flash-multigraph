<mugl>
  <plotarea marginleft="50"/>
 <horizontalaxis type="datetime">
   <labels>
     <label format="%n" spacing="1M"/>
     <label format="%n %d" spacing="1M 7D 3D 2D 1D"/>
     <label format="%n %d %h:00" spacing="12H 6H 3H 2H 1H"/>
   </labels>
   <grid/>
 </horizontalaxis>
 <verticalaxis id="tempf" type="number">
   <labels format="%.2f"  spacing="50 20 10 5 2 1 0.5 0.2 0.1 0.05 0.02 0.01"/>
   <grid/>
   <title angle="90" position="-40 0" anchor="0 -1">Temperature (F)</title>
 </verticalaxis>
 <plot>
    <legend label="Temperature"/>
    <horizontalaxis ref="xaxis">
      <variable ref="year"/>
    </horizontalaxis>
    <verticalaxis ref="tempf">
      <variable ref="tempf"/>
    </verticalaxis>
    <renderer type="pointline">
    </renderer>
   </plot>
   <data>
    <variables>
     <variable id="year" column="0" type="datetime"/>
     <variable id="tempf" column="1"/>
     <variable id="tempc" column="2"/>
     <variable id="dpf" column="3"/>
     <variable id="dpc" column="4"/>
     <variable id="pi" column="5"/>
     <variable id="phpa" column="6"/>
     <variable id="winddir" column="7"/>
     <variable id="windspd" column="8"/>
    </variables>
    <csv location="http://services.multigraph.org/asos/?station=KGSP," randomize="true"/>
  </data>
</mugl>
