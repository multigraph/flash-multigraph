<mx:Application
  xmlns:mx="http://www.adobe.com/2006/mxml"
  xmlns:multigraph="multigraph.*"
  layout="absolute"
  backgroundColor="0xFFFFFF"
  backgroundGradientColors="undefined"
  applicationComplete="init()"
  horizontalAlign="left"
  paddingLeft="0"
  paddingTop="0"
  paddingBottom="0"
  paddingRight="0"
  width="100%"
  height="100%"
>

  <mx:Script><![CDATA[
	private function init():void {
		stage.scaleMode = StageScaleMode.NO_SCALE;
    	stage.align = StageAlign.TOP_LEFT;
		//stage.focus = mgraph;
        //mgraph.init();
	}
  ]]></mx:Script>

  <multigraph:Multigraph id="mgraph"
     test="{parameters['test']}"
     rendererlist="{parameters['rendererlist']}"
     swfname="{parameters['swfname']}"
     hostname="{parameters['hostname']}"
     pathname="{parameters['pathname']}"
     port="{parameters['port']}"
     proxy="{parameters['proxy']}"
     muglfile="{parameters['muglfile']}"
   >
   </multigraph:Multigraph>
</mx:Application>

